set number
set tabstop=4
set shiftwidth=4
set autoindent
set mouse=a
set smartindent
set wildmenu
set wildmode=list:longest
set showmode

command! PS :silent !start powershell > /dev/null 2>&1

filetype on
filetype plugin on
filetype indent on
syntax on
set cursorcolumn
filetype plugin indent on

" https://github.com/prettier/vim-prettier
let g:prettier#config#bracket_spacing = 'true'
let g:prettier#config#jsx_bracket_same_line = 'false'
autocmd BufWritePre *.js,*.jsx,*.mjs,*.ts,*.tsx,*.css,*.less,*.scss,*.json,*.graphql,*.md,*.vue,*.yaml,*.html Prettier

" The functions MarkDownSwap() & DoWindowSwap() are for swapping active windows quickly back and forth using 
" HJKL based on where the other split windows are located on the screen relative to your current active vim window.
"
function! MarkWindowSwap()
	" marked window number
	let g:markedWinNum = winnr()
	let g:markedBufNum = bufnr("%")
endfunction

function! DoWindowSwap()
	let curWinNum = winnr()
	let curBufNum = bufnr("%")
	" Switch focus to marked window
	exe g:markedWinNum . "wincmd w"

	" Load current buffer on marked window
	exe 'hide buf' curBufNum

	" Switch focus to current window
	exe curWinNum . "wincmd w"

	" Load current buffer on marked window
	exe 'hide buf' g:markedBufNum
endfunction

nnoremap H :call MarkWindowSwap()<CR> <C-w>h :call DoWindowSwap()<CR>
nnoremap J :call MarkWindowSwap()<CR> <C-w>j :call DoWindowSwap()<CR>
nnoremap K :call MarkWindowSwap()<CR> <C-w>k :call DoWindowSwap()<CR>
nnoremap L :call MarkWindowSwap()<CR> <C-w>l :call DoWindowSwap()<CR>

function! ToggleTerminal()
	if exists('t:term_buf_nr')
		let l:bufnr = bufnr('^term://.*$')
		if l:bufnr != -1
			execute 'bdelete' l:bufnr
			unlet t:term_buf_nr
		endif
	else
		execute 'terminal'
		let t:term_buf_nr = bufnr('%')
	endif
endfunction

nnoremap <silent> <M-t> :call ToggleTerminal()<CR>
nnoremap <space> :
inoremap jj <esc>

" # Close brackets automatically with return
inoremap {<cr> {<cr>}<C-O><S-O>
inoremap (<cr> (<cr>)<C-O><S-O>
inoremap [<cr> [<cr>]<C-O><S-O>
" # Two cases below are covered by inoramp <exp>
inoremap ( ()<left>
inoremap { {}<left>
inoremap [ []<left>
inoremap <expr> ) strpart(getline('.'), col('.'), col('.')-1,1) == ")" ? "\<Right>" : ")"
inoremap <expr> } strpart(getline('.'), col('.'), col('.')-1,1) == "}" ? "\<Right>" : "}"
inoremap <expr> ] strpart(getline('.'), col('.'), col('.')-1,1) == "]" ? "\<Right>" : "]"
inoremap <expr> ' strpart(getline('.'), col('.'), col('.')-1,1) == "'" ? "\<Right>" : "''<left>"
inoremap <expr> " strpart(getline('.'), col('.'), col('.')-1,1) == "\"" ? "\<Right>" : "\"\"<left>"

call plug#begin('~/.vim/plugged')

Plug 'neoclide/coc.nvim', {'branch':'release'}
Plug 'preservim/nerdtree'
Plug 'rust-lang/rust.vim'
Plug 'prettier/vim-prettier', {
	\ 'do': 'np, install',
	\ 'for': ['javascript', 'typescript', 'css','less','scss','json','graphql','markdown','vue','yaml','html'] }		

call plug#end()

set statusline=
set statusline+=\ %F\ %M\ %Y\ %R

